# This is Git's per-user configuration file.
[user]
# Please adapt and uncomment the following lines:
  name = Nick Ray
  email = nick@underbelly.is
[push]
  default = current
[color]
  ui = auto
[core]
  excludesfile = ~/.gitignore
  autocrlf = input
[merge]
  ff = only
[commit]
  template = ~/.gitmessage
[fetch]
  prune = true
[rebase]
  autosquash = true
